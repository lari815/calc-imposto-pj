            function calculo_ir(){   if (lucro_presumido > 240000) {     var ir = parseInt(lucro_presumido) * 0.15 + (parseInt(lucro_presumido) - 240000) * 0.1;   }   else {     var ir = parseInt(lucro_presumido) * 0.15;   }  }
            

