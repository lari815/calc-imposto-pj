            function define_lp(){
                if ramo == ("at_imob" || "com" || "ind_terc" "transp_cargo");
                var classe = "classe1";
                else if ramo == ("prest_serv");
                var classe = "classe2";
                else if ramo == ("rev_comb");
                var classe = "classe3";
                else if ramo == ("serv_hosp");
                var classe = "classe4";
                else if ramo == ("transp");
                var classe = "classe5";
            }

            function calculo_lp(){
